@model IEnumerable<Beautydemo.Models.tb_admin>

@{
    ViewBag.Title = "Index";
}

<h2>管理员首页</h2>

<p>
    @Html.ActionLink("新增管理员", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.aname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.confirmpassword)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tel)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.aname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.confirmpassword)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tel)
        </td>
        <td>
            @Html.ActionLink("编辑", "Edit", new { id=item.aid }) |
            @Html.ActionLink("详情", "Details", new { id=item.aid }) |
            @Html.ActionLink("删除", "Delete", new { id=item.aid })
        </td>
    </tr>
}

</table>
